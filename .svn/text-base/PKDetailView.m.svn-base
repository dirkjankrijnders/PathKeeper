#import "PKDetailView.h"


@implementation PKDetailView

- (void)drawRect:(NSRect)dirtyRect {
	CGFloat per = 0.85;
	[[NSColor colorWithDeviceRed:per green:per blue:per alpha: 1.0] set];
	[NSBezierPath fillRect: self.bounds];
}

- (BOOL) isOpaque
{
	return YES;
}

@end
